        "work_title": "Agua de látex",
        "popup_size": "Small",
        "popup_position": "Top Left",
        "artist_name": "Kira Xonorika",
        "creation_date": "2023",
        "medium": "AI, still image",
        "createdAt": "2024-09-27T12:52:51.966Z",
